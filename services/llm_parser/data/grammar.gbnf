alternative-0 ::= specObject
alternative-1 ::= specificationsList
array ::= "[" space ( value ("," space value)* )? "]" space
boolean ::= ("true" | "false") space
char ::= [^"\\\x7F\x00-\x1F] | [\\] (["\\bfnrt] | "u" [0-9a-fA-F]{4})
decimal-part ::= [0-9]{1,16}
integral-part ::= [0] | [1-9] [0-9]{0,15}
null ::= "null" space
number ::= ("-"? integral-part) ("." decimal-part)? ([eE] [-+]? integral-part)? space
object ::= "{" space ( string ":" space value ("," space string ":" space value)* )? "}" space
root ::= alternative-0 | alternative-1
space ::= | " " | "\n"{1,2} [ \t]{0,20}
specObject ::= "{" space specObject-category-kv "," space specObject-details-kv "}" space
specObject-category-kv ::= "\"category\"" space ":" space string
specObject-details ::= specObject-details-0 | specObject-details-1
specObject-details-0 ::= "[" space (string ("," space string)*)? "]" space
specObject-details-1 ::= object
specObject-details-kv ::= "\"details\"" space ":" space specObject-details
specificationsList ::= "[" space (specificationsList-item ("," space specificationsList-item)*)? "]" space
specificationsList-item ::= specObject
string ::= "\"" char* "\"" space
value ::= object | array | string | number | boolean | null
