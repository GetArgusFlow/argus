char ::= [^"\\\x7F\x00-\x1F] | [\\] (["\\bfnrt] | "u" [0-9a-fA-F]{4})
color ::= string | null
color-kv ::= "\"color\"" space ":" space color
decimal-part ::= [0-9]{1,16}
dimensions ::= string | null
dimensions-kv ::= "\"dimensions\"" space ":" space dimensions
ean ::= string | null
ean-kv ::= "\"ean\"" space ":" space ean
height ::= number | null
height-kv ::= "\"height\"" space ":" space height
integer ::= ("-"? integral-part) space
integral-part ::= [0] | [1-9] [0-9]{0,15}
