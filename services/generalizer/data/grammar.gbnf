null ::= "null" space
number ::= ("-"? integral-part) ("." decimal-part)? ([eE] [-+]? integral-part)? space
price ::= number | null
price-kv ::= "\"price\"" space ":" space price
product-type ::= string | null
product-type-kv ::= "\"product_type\"" space ":" space product-type
root ::= "{" space brand-kv "," space brand-type-kv "," space product-type-kv "," space price-kv "," space material-kv "," space color-kv "," space height-kv "," space width-kv "," space length-kv "," space dimensions-kv "," space weight-kv "," space net-volume-kv "," space net-volume-unit-kv "," space capacity-kv "," space area-kv "," space specifications-kv "," space item-count-kv "," space item-unit-kv "," space category-kv "," space ean-kv "}" space
space ::= | " " | "\n"{1,2} [ \t]{0,20}
